{
  "name": "enable-button-icons",
  "version": "0.1.2",
  "description": "Add icons to Button blocks.",
  "author": "Nick Diego, Paul Eiche",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "git://github.com/peiche/enable-button-icons.git"
  },
  "scripts": {
    "build": "npm run lint && wp-scripts build",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:src": "wp-scripts lint-js ./src",
    "lint:js:src:fix": "wp-scripts lint-js ./src --fix",
    "lint": "npm run lint:css && npm run lint:js:src",
    "start": "wp-scripts start",
    "packages-update": "wp-scripts packages-update",
    "update-pot": "wp i18n make-pot . languages/enable-button-icons.pot --exclude=src"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@wordpress/icons": "^9.42.0",
    "@wordpress/scripts": "^27.2.0",
    "webpack-remove-empty-scripts": "^0.8.4"
  }
}
